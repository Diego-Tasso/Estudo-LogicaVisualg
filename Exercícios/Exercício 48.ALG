Algoritmo "48"

Var
   i , g, soma, numero : inteiro

Inicio
   i <- 1
   Enquanto ( i <= 7) faca
      Escreval ("Número: " , i )
      Escreva ("Me diga o valor do número: ")
      Leia (numero)
      soma <- soma + numero
      i <- i + 1
   FimEnquanto
   Escreval ("O valor da soma é: " , soma)



Fimalgoritmo