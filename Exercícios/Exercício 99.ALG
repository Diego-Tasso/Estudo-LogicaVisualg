Algoritmo "99"

Var
   numA, numB, res: real

Funcao Potencia (a, b : real) : real
Inicio

   Retorne (Exp(a,b))
FimFuncao
Inicio
   Escreva ("Me diga o valor da base --> ")
   Leia (numA)
   Escreva ("Me diga o valor do expoente --> ")
   Leia (numB)

   res <- Potencia (numA, numB)
   Escreva ("A Potencia equivale a --> " , res)



Fimalgoritmo