Algoritmo "56"

Var
   numero, totalN, i: inteiro


Inicio
      i <- 1
      Enquanto (nao(numero = 1111)) faca
               Escreva ("Qual o valor do número: ")
               Leia (numero)
               totalN <- totalN + numero
               LimpaTela
               i <- i + 1
      Fimenquanto
      Escreva ("O somatório entre os números é: " ,  totalN - 1111)



Fimalgoritmo